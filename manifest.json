{
    "name": "Discover Studios Extension",
    "manifest_version" : 2,
    "version": "1.0",
    "description": "MyExtension",
    "icons": { "16": "discoverstudios-16px.png",
               "48": "discoverstudios-48px.png",
              "128": "discoverstudios-128px.png" 
    },
    "browser_action": {
        "default_icon": "discoverstudios-16px.png",
        "default_popup": "my.html"
    },
    "chrome_url_overrides": {
        "newtab": "my.html"
    },
    "permissions": [
        "tabs"
    ]
}